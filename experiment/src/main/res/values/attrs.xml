<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="ResourceName">
    <declare-styleable name="MultiImageView">
        <attr name="miv_images" format="reference" />
        <attr name="miv_position" format="integer" />
    </declare-styleable>

    <declare-styleable name="SuperItemView">
        <attr name="siv_layout" format="reference" />
        <attr name="siv_image" format="reference" />
        <attr name="siv_text" format="string" />
        <attr name="siv_second_icon" format="reference" />
        <attr name="siv_second_text" format="string" />
        <attr name="siv_checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RatioLayout">
        <attr name="rfl_ratio" format="float" />
    </declare-styleable>

    <declare-styleable name="ShadowFrameLayout">
        <attr name="sfl_shadow_color" format="color" />
        <attr name="sfl_shadow_depth" format="dimension" />
        <attr name="sfl_shadow_round" format="dimension" />
        <attr name="sfl_shadow_left_offset" format="dimension" />
        <attr name="sfl_shadow_top_offset" format="dimension" />
        <attr name="sfl_shadow_right_offset" format="dimension" />
        <attr name="sfl_shadow_bottom_offset" format="dimension" />
    </declare-styleable>
</resources>